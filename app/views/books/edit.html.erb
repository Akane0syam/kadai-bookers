<h1>Editing Book</h1>
<%= form_with model: @book, irl: update_book_path(@book.id),method: :patch do |f| %>
  <h5>Title</h5>
  <%= f.text_field :title %>
  <h5>Body</h5>
  <%= f.text_area :body %>

  <%= f.submit 'Update Book'%>
<% end %>

